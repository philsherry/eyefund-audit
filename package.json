  "main": "index.js",
  "name": "audit-template",
  "nodeW3Cvalidator": {
    "suppressErrors": [],
    "suppressWarnings": []
  },
  "pre-commit": [
    "precommit-msg",
    "lint:tests"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/philsherry/audit-template.git"
  },
  "scripts": {
    "babelup": "npx babel-upgrade --write",
    "browserslist": "npx browserslist@latest --update-db",
    "cy:clear": "cypress cache clear",
    "cy:install": "cypress install",
    "cy:open": "cypress open",
    "cy:run": "cypress run --reporter mochawesome --reporter-options reportDir='cypress/results',overwrite=false,html=true,json=true  ",
    "hint": "hint http://localhost/",
    "html": "html-validate --config .htmlvalidate.json --",
    "html:all:console": "html-validate --config .htmlvalidate.json './audit/**/*.html'",
    "html:all:txt": "html-validate --config .htmlvalidate.json './audit/**/*.html' --formatter text=./reports/w3c/html-validate.txt",
    "htmlhint": "htmlhint --config .htmlhintrc --",
    "htmlhint:all": "htmlhint --config .htmlhintrc './audit/**/*.html'",
    "imgs": "imageoptim --imagealpha './audit/screenshots/**/*.png'",
    "lh": "run-s lh:desktop lh:mobile",
    "lh:desktop": "lighthouse  --output-path ./reports/lighthouse/desktop --output json --output html --config-path=./config/lighthouse/desktop.conf.js",
    "lh:mobile": "lighthouse  --output-path ./reports/lighthouse/mobile --output json --output html --config-path=./config/lighthouse/mobile.conf.js",
    "lint": "run-s lint:js lint:css",
    "lint:js": "eslint './audit/**/*.js'",
    "lint:css": "stylelint 'audit/**/*.{css,scss}'",
    "lint:fix:js": "eslint --fix './audit/**/*.js'",
    "lint:fix:css": "stylelint --fix 'audit/**/*.{css,scss}'",
    "lint:fix:tests": "eslint --fix './cypress/e2e/**/*.js'",
    "localhost": "http-server -o ./examples -p 1337",
    "pa11y": "pa11y --",
    "pa11y:all": "run-s pa11y:ci pa11y:html",
    "pa11y:ci": "pa11y-ci --json --config .pa11y-ci.json > ./audit/data/pa11y-ci-results.json 2>&1",
    "pa11y:html": "pa11y-ci-reporter-html --include-zero-issues --source ./audit/data/pa11y-ci-results.json --destination ./audit/pa11y-ci/",
    "playwrite": "npx playwright install",
    "postinstall": "run-s playwrite cy:install",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "pretest": "run-s browserslist",
    "start": "run-s validate:all html:all:console pa11y:all imgs",
    "stylelint": "stylelint --config .stylelintrc --",
    "stylelint:all": "stylelint --config .stylelintrc './audit/**/*.css'",
    "test": "cy:run",
    "validate": "node-w3c-validator -f lint -i",
    "validate:all": "node-w3c-validator -f html --verbose -o './reports/w3c/report.html' --html -i './audit/**/*.html'",
    "validate:all:console": "node-w3c-validator -f lint --verbose --html -i './audit/**/*.html' -o './audit/w3c/report.html'",
    "validate:html": "node-w3c-validator --buffersize 2097152 --format html --html --no-stream --output './reports/w3c/node-w3c-validator.html' --verbose --input"
  },
  "version": "1.0.0",
  "dependencies": {
    "@babel/preset-react": "^7.18.6",
    "@babel/register": "^7.18.9",
    "babel-plugin-transform-class-properties": "^6.24.1"
  }
